    "version": "0.2.0",
    "configurations": [
      {
        "name": "AzureFunctions",
        "type": "coreclr",
        "request": "attach",
        "processId": "${command:azureFunctions.pickProcess}"
      },
      {
        "name": "Console",
        "type": "coreclr",
        "request": "launch",
        "preLaunchTask": "build",
        "program": "${workspaceFolder}/Example.Console/bin/Debug/net5.0/Example.Console.dll",
        "args": [],
        "cwd": "${workspaceFolder}/Example.Console",
        "console": "externalTerminal",
        "stopAtEntry": false
      },
      {
        "name": "Web",
        "type": "coreclr",
        "request": "launch",
        "preLaunchTask": "build-web",
        "program": "${workspaceFolder}/Example.Web/bin/Debug/net5.0/Example.Web.dll",
        "args": [],
        "cwd": "${workspaceFolder}/Example.Web",
        "console": "externalTerminal",
        "stopAtEntry": false
      },
      {
        "name": "WebApi",
        "type": "coreclr",
        "request": "launch",
        "preLaunchTask": "build-webapi",
        "program": "${workspaceFolder}/Example.WebApi/bin/Debug/net5.0/Example.WebApi.dll",
        "args": [],
        "cwd": "${workspaceFolder}/Example.WebApi",
        "console": "externalTerminal",
        "stopAtEntry": false
      },
      {
        "name": "MvcRazor",
        "type": "coreclr",
        "request": "launch",
        "preLaunchTask": "build-mvc-razor",
        "program": "${workspaceFolder}/Example.Mvc.Razor/bin/Debug/net5.0/Example.Mvc.Razor.dll",
        "args": [],
        "cwd": "${workspaceFolder}/Example.Mvc.Razor",
        "console": "externalTerminal",
        "stopAtEntry": false
      },
      {
        "name": "Mvc",
        "type": "coreclr",
        "request": "launch",
        "preLaunchTask": "build-mvc",
        "program": "${workspaceFolder}/Example.Mvc.Controllers/bin/Debug/net5.0/Example.Mvc.Controllers.dll",
        "args": [],
        "cwd": "${workspaceFolder}/Example.Mvc.Controllers",
        "console": "externalTerminal",
        "stopAtEntry": false
      }
    ]
}