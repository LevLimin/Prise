    "version": "0.2.0",
    "configurations": [
      {
        "name": "AzureFunctions",
        "type": "coreclr",
        "request": "attach",
        "processId": "${command:azureFunctions.pickProcess}"
      },
      {
        "name": "Console",
        "type": "coreclr",
        "request": "launch",
        "preLaunchTask": "build",
        "program": "${workspaceFolder}/Prise.Example.Console/bin/Debug/netcoreapp3.1/Prise.Example.Console.dll",
        "args": [],
        "cwd": "${workspaceFolder}/Prise.Example.Console",
        "console": "externalTerminal",
        "stopAtEntry": false
      },
      {
        "name": "Web",
        "type": "coreclr",
        "request": "launch",
        "preLaunchTask": "build-web",
        "program": "${workspaceFolder}/Prise.Example.Web/bin/Debug/netcoreapp3.1/Prise.Example.Web.dll",
        "args": [],
        "cwd": "${workspaceFolder}/Prise.Example.Web",
        "console": "externalTerminal",
        "stopAtEntry": false
      },
      {
        "name": "Mvc",
        "type": "coreclr",
        "request": "launch",
        "preLaunchTask": "build-mvc",
        "program": "${workspaceFolder}/Prise.Example.Razor/bin/Debug/net5.0/Prise.Example.Razor.dll",
        "args": [],
        "cwd": "${workspaceFolder}/Prise.Example.Razor",
        "console": "externalTerminal",
        "stopAtEntry": false
      }
    ]
}