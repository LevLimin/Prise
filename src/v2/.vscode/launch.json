    "version": "0.2.0",
    "configurations": [
        {
            "name": "Prise.Console",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/Prise.Console/bin/Debug/netcoreapp3.1/Prise.Console.dll",
            "args": [],
            "cwd": "${workspaceFolder}/Prise.Console",
            "console": "externalTerminal",
            "stopAtEntry": false
        },
        {
            "name": "Prise.Web",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build-web",
            "program": "${workspaceFolder}/Prise.Web/bin/Debug/netcoreapp3.1/Prise.Web.dll",
            "args": [],
            "cwd": "${workspaceFolder}/Prise.Web",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}